{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current",
          "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
        }
      }
    ]
  ],
  "plugins": [
    "transform-runtime",
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    "transform-decorators-legacy"
  ]
}
